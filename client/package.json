{
  "name": "client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:prod": "cross-env NODE_ENV=production eslint .",
    "lint:prod:fix": "cross-env NODE_ENV=production eslint . --fix",
    "lint:release": "cross-env NODE_ENV=production RELEASE=true eslint .",
    "lint:release:fix": "cross-env NODE_ENV=production RELEASE=true eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "build": "tsc -b && vite build",
    "build:staging": "tsc -b && vite build --mode staging",
    "build:production": "tsc -b && vite build --mode production",
    "preview": "vite preview",
    "prebuild": "npm run lint:prod && npm run format:check",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "test:silent": "vitest run --silent"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.90.5",
    "@tanstack/react-query-devtools": "^5.90.2",
    "axios": "^1.12.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.9.4",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^5.0.0",
    "babel-plugin-react-compiler": "^19.1.0-rc.3",
    "cross-env": "^7.0.3",
    "eslint": "^9.33.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^6.1.1",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "prettier": "^3.3.3",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "vite": "^7.1.2",
    "vitest": "^4.0.1"
  }
}
